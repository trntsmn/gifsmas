{
  "name": "gifsmas",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "postinstall": "./node_modules/bower/bin/bower install"
  },
  "engines": {
    "node": "5.0.x",
    "npm": "3.3.x"
  },
  "dependencies": {
    "body-parser": "~1.13.2",
    "bower": "^1.6.5",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "^4.13.3",
    "express-handlebars": "^2.0.1",
    "express-hbs": "^0.8.4",
    "hbs": "~3.1.0",
    "morgan": "~1.6.1",
    "multer": "^1.1.0",
    "node-sass-middleware": "0.8.0",
    "serve-favicon": "~2.3.0"
  },
  "devDependencies": {
    "browser-sync": "^2.10.0",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-cache": "^0.4.0",
    "gulp-concat": "^2.6.0",
    "gulp-express": "^0.3.5",
    "gulp-livereload": "^3.8.1",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^2.1.0",
    "gulp-uglify": "^1.4.2"
  },
  "webroot": "public",
  "webapp": "public/app",
